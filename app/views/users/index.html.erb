<h1>The User_Derpers List of Magical Users</h1>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th colspan="2"></th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.surname %></td>
      <td><%= user.email %></td>
    </tr>
  <% end %>
</table>

